
@{
    ViewBag.Title = "DeneyimEkle";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
<br />
<h4>Yeni Deneyim Ekleme Sayfası</h4>
<form class="form-group" method="post">
    <input type="text" name="Baslik" class="form-control" placeholder="Başlık Giriniz" /><br />
    <input type="text" name="AltBaslik" class="form-control" placeholder="Alt Başlık Giriniz" /><br />
    <input type="text" name="Tarih" class="form-control" placeholder="Çalışma Tarihi Giriniz" /><br />
    <textarea placeholder="Açıklamanızı Giriniz" name="Aciklama" class="form-control" rows="5"></textarea><br />
    <button class="btn btn-info">Kaydet</button>
</form>

